{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "allowImportingTsExtensions": true
  },
  "include": ["./**/*.ts", "routes/gifts.js"],
  "exclude": ["node_modules"]
}
